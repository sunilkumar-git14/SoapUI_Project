                <scriptText>//script assertions for xml response
//below import stmt is used to extract the xml type of response
import com.eviware.soapui.support.XmlHolder

//def is keyword used to define variable
//getting xml response into a variable
def xmlresponse=messageExchange.response.responseContent

//here parsing xml response so can get the specific node from parsed xml
def parsexmlresponse=new XmlHolder(xmlresponse)

//getting specific node
//defining namespace here
parsexmlresponse.namespaces["ns"]="http://tempuri.org/"

//why we are defining names space here names in output tags are xmlns(xml namespace) so
//getting specific node
//assinging namespace here and getting node response
//log.info is used to display the value
log.info parsexmlresponse.getNodeValue("//ns:AddResponse/ns:AddResult")

//another way of overcoming namespace here
def actsum=parsexmlresponse.getNodeValue("//*:AddResponse/*:AddResult")

//defining expected value as property
//testcase level property
def expsum=context.getTestCase().getPropertyValue("expsum")

//testsuite property level
//context.getTestCase().getTestSuite().getPropertyValue("expsum")

assert actsum==expsum</scriptText>
